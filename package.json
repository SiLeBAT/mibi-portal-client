{
  "name": "mibi-portal-client",
  "version": "2.1.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "serve": "ng serve --proxy-config proxy.config.json --watch --hmr",
    "debug": "ng serve -o --proxy-config proxy.config.json --watch",
    "serve:production": "ng serve --proxy-config proxy.config.json --prod=true",
    "build:qa": "ng build --configuration=qa --output-path \"public\"",
    "prebuild:qa": "npm run clean",
    "build:production": "ng build --configuration=production --output-path \"public\"",
    "prebuild:production": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run --spec 'cypress/integration/**/*.spec.ts'",
    "reports:delete": "npx rimraf cypress/reports/* || true",
    "reports:create": "npx mochawesome-merge cypress/reports/mocha/mochawesome*.json > cypress/reports/mocha/merged-report.json && npx mochawesome-report-generator ./cypress/reports/mocha/merged-report.json -o ./cypress/reports/mocha -t \"mibi-portal e2e\" -p \"mibi-portal e2e\" --charts --timestamp \"isoDateTime\" --showSkipped",
    "pretest:e2e": "npm run reports:delete",
    "posttest:e2e": "npm run reports:create",
    "lint": "npm run lint:code && npm run lint:styles",
    "lint:code": "ng lint",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "clean": "npx rimraf public/*",
    "cypress:open": "cypress open",
    "prepare": "(husky -v && husky install) || echo husky not installed"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix --fix-type layout"
    ],
    "src/**/*.html": [
      "eslint --fix --fix-type layout"
    ],
    "src/**/*.scss": [
      "stylelint --fix"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "~11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/material": "~11.2.13",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@auth0/angular-jwt": "~5.0.2",
    "@ngrx/effects": "~11.1.1",
    "@ngrx/router-store": "~11.1.1",
    "@ngrx/store": "~11.1.1",
    "@ngrx/store-devtools": "~11.1.1",
    "angular-file": "~3.6.0",
    "angular-password-strength-meter": "~2.0.0",
    "file-saver": "^2.0.5",
    "jquery": "~3.6.0",
    "lodash": "~4.17.21",
    "moment": "~2.29.2",
    "ngx-moment": "~5.0.0",
    "normalize.css": "~8.0.1",
    "rxjs": "~6.6.7",
    "tooltipster": "~4.2.8",
    "tslib": "~2.3.1",
    "zone.js": "0.11.4",
    "zxcvbn": "~4.4.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular-eslint/builder": "^4.3.1",
    "@angular-eslint/schematics": "^4.3.1",
    "@angular/cli": "~11.2.19",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@bahmutov/add-typescript-to-cypress": "~2.1.2",
    "@ngrx/schematics": "~11.1.1",
    "@types/file-saver": "2.0.5",
    "@types/jest": "27.0.1",
    "@types/lodash": "4.14.181",
    "@types/tooltipster": "0.0.31",
    "@types/zxcvbn": "4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "cypress": "~9.3.1",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^7.32.0",
    "eslint-plugin-rxjs": "~3.3.7",
    "eslint-plugin-unicorn": "~40.1.0",
    "husky": "~7.0.4",
    "jest": "~27.1.1",
    "jest-preset-angular": "~9.0.7",
    "lint-staged": "~11.1.4",
    "mocha": "~8.1.3",
    "mocha-junit-reporters": "~1.23.6",
    "mochawesome": "~6.1.1",
    "mochawesome-merge": "~4.1.0",
    "rimraf": "~3.0.2",
    "stylelint": "~13.6.1",
    "stylelint-config-standard": "~20.0.0",
    "typescript": "4.0.8"
  },
  "mibiConfig": {
    "lastChange": "2019-04-16 11:25:17 +0200"
  }
}