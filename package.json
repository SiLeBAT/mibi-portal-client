{
  "name": "mibi-portal-client",
  "version": "3.0.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "serve": "ng serve --proxy-config proxy.config.json --watch --hmr",
    "debug": "ng serve -o --proxy-config proxy.config.json --watch",
    "build": "ng build --configuration=development",
    "build:qa": "ng build --configuration=qa",
    "build:production": "ng build --configuration=production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run --spec 'cypress/integration/**/*.spec.ts'",
    "reports:delete": "rimraf cypress/reports/* || true",
    "reports:create": "mochawesome-merge cypress/reports/mocha/mochawesome*.json > cypress/reports/mocha/merged-report.json && marge ./cypress/reports/mocha/merged-report.json -o ./cypress/reports/mocha -t \"mibi-portal e2e\" -p \"mibi-portal e2e\" --charts --timestamp \"isoDateTime\" --showSkipped",
    "pretest:e2e": "npm run reports:delete",
    "posttest:e2e": "npm run reports:create",
    "lint": "npm run lint:code && npm run lint:styles",
    "lint:code": "ng lint",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "cypress:open": "cypress open",
    "prepare": "(husky -v && husky ) || echo husky not installed",
    "lint:fix": "npm run lint:code -- --fix && npm run lint:styles -- --fix"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint"
    ],
    "src/**/*.html": [
      "eslint"
    ],
    "src/**/*.scss": [
      "stylelint"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@auth0/angular-jwt": "~5.0.2",
    "@fontsource/material-icons": "4.5.4",
    "@fontsource/roboto": "4.5.8",
    "@ngrx/effects": "^14.3.3",
    "@ngrx/router-store": "^14.3.3",
    "@ngrx/store": "^14.3.3",
    "@ngrx/store-devtools": "^14.3.3",
    "angular-file": "^4.0.2",
    "angular-password-strength-meter": "~5.1.0",
    "file-saver": "^2.0.5",
    "jquery": "~3.6.0",
    "lodash": "~4.17.21",
    "moment": "~2.29.4",
    "ngx-moment": "~6.0.2",
    "normalize.css": "~8.0.1",
    "rxjs": "~6.6.7",
    "tooltipster": "~4.2.8",
    "tslib": "~2.3.1",
    "zone.js": "0.11.4",
    "zxcvbn": "~4.4.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.12",
    "@angular-eslint/builder": "^14.4.0",
    "@angular-eslint/schematics": "^14.4.0",
    "@angular-eslint/template-parser": "^14.4.0",
    "@angular/cli": "^14.2.12",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@angular/localize": "^14.3.0",
    "@ngrx/schematics": "~14.3.3",
    "@types/file-saver": "2.0.5",
    "@types/jest": "^28.1.8",
    "@types/lodash": "4.14.182",
    "@types/tooltipster": "0.0.31",
    "@types/zxcvbn": "4.4.1",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "cypress": "^13.6.3",
    "cypress-multi-reporters": "^1.6.0",
    "eslint": "^8.27.0",
    "eslint-plugin-rxjs": "~5.0.2",
    "eslint-plugin-unicorn": "~40.1.0",
    "husky": "~9.0.0",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.6",
    "lint-staged": "~11.1.4",
    "mocha": "~10.2.0",
    "mocha-junit-reporters": "~1.23.6",
    "mochawesome": "~7.1.3",
    "mochawesome-merge": "~4.3.0",
    "rimraf": "~3.0.2",
    "stylelint": "~14.8.2",
    "stylelint-config-standard-scss": "~3.0.0",
    "typescript": "4.6.4"
  },
  "overrides": {
    "follow-redirects": "1.15.4"
  },
  "mibiConfig": {
    "lastChange": "2019-04-16 11:25:17 +0200"
  }
}
